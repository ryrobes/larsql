{
  "name": "windlass-rabbitize",
  "version": "0.2.0",
  "description": "Embedded browser automation server for RVBBIT - forked from Rabbitize",
  "main": "src/index.js",
  "bin": {
    "windlass-rabbitize": "src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "playwright install chromium",
    "lint": "eslint src/"
  },
  "keywords": [
    "windlass",
