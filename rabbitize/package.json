    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryrobes/windlass.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "express": "^4.18.2",
    "figlet": "^1.8.0",
    "node-fetch": "^2.7.0",
    "pidusage": "^3.0.2",
    "pixelmatch": "^6.0.0",
